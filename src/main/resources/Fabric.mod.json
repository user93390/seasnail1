{
  "schemaVersion": 1,
  "id": "addon-template",
  "version": "${version}",
  "name": "Addon Template",
  "description": "An addon template for Meteor Client addons.",
  "authors": [
    "seasnail"
  ],
  "contact": {
    "repo": "https://github.com/user93390/seasnail1"
  },
  "icon": "assets/template/icon.png",
